					StrokeThickness="4"/> 
			</Custom:SurfaceToggleButton> 
            -->
            <!--
			<Custom:SurfaceToggleButton x:Name="btnArrow" Background="{x:Null}" Width="60" Height="60" Click="btnArrow_Click">
