                           FontFamily="Segoe UI" FontWeight="Bold" />
                </Canvas>
            </Custom:SurfaceToggleButton>
            <!--<Custom:SurfaceToggleButton x:Name="btnReset" Background="Transparent" Click="btnReset_Click" Width="60"
