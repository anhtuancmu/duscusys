<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="MainBgrndRctFill" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF500064" Offset="0" />
        <GradientStop Color="#FF590768" Offset="1" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="MainBgrndRctStroke" Color="#FF3C1441" />
    <RadialGradientBrush x:Key="TileBgrndRctFill" GradientOrigin="0.582,0.409">
        <RadialGradientBrush.RelativeTransform>
            <TransformGroup>
                <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleX="2.525" ScaleY="2.525" />
                <SkewTransform AngleX="0" AngleY="0" CenterX="0.5" CenterY="0.5" />
                <RotateTransform Angle="0" CenterX="0.5" CenterY="0.5" />
                <TranslateTransform X="0.378" Y="-0.378" />
            </TransformGroup>
        </RadialGradientBrush.RelativeTransform>
        <GradientStop Color="#FF890D9F" Offset="0" />
        <GradientStop Color="#FF710684" Offset="1" />
    </RadialGradientBrush>
    <!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>